{
	"extends": "airbnb",
  "rules": {
    "arrow-body-style": 0,
    "func-names": 0,
    "no-unused-vars": 1,
    "no-undef": 0,
    "prefer-template": 0,
    "quote-props": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 1,
    "no-plusplus": [2, { "allowForLoopAfterthoughts": true }],
    "no-shadow": 0
  }
}
